{
  "name": "calculator-extension",
  "displayName": "Calculator Extension",
  "description": "A simple calculator extension for Visual Studio Code.",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "calculatorExtension",
          "title": "Calculator",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "calculatorExtension": [
        {
          "id": "calculatorExtension.view",
          "name": "Calculator"
        }
      ]
    },
    "commands": [
      {
        "command": "calculatorExtension.calculate",
        "title": "Calculator: Calculate"
      }
    ]
  },
  "activationEvents": [
    "onCommand:calculatorExtension.calculate"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Giovasdf/calculatorextension-vscode.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.58.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.32.0",
    "typescript": "^4.4.4",
    "vscode-test": "^1.6.0"
  },
  "keywords": [
    "vscode",
    "calculator",
    "extension"
  ],
  "license": "MIT",
  "author": {
    "name": "Giovanni Molina",
    "email": "tuemail@example.com",
    "url": "https://github.com/Giovasdf"
  },
  "bugs": {
    "url": "https://github.com/Giovasdf/calculatorextension-vscode/issues"
  },
  "homepage": "https://github.com/Giovasdf/calculatorextension-vscode"
}
